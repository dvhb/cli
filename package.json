  },
  "scripts": {
    "init": "bin/cli.js init",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:init": "node fixture.js",
    "start": "npm run dev:basic",
    "dev:basic": "bin/cli.js server --config examples/basic/dvhb.config.js --verbose",
    "dev:static-site": "bin/cli.js server --config examples/static-site/dvhb.config.js --verbose",
    "dev:spa-angular": "bin/cli.js server --config examples/spa-angular/dvhb.config.js --verbose",
    "start:static-site": "bin/cli.js server --config examples/static-site/dvhb.config.js --env=production",
    "prestart:static-site": "npm run build:static-site",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "lint-staged": "lint-staged",
    "build:basic": "bin/cli.js build --config examples/basic/dvhb.config.js --verbose --app-env=development",
    "build:static-site": "bin/cli.js build --config examples/static-site/dvhb.config.js --verbose --app-env=development",
    "build:spa-angular": "bin/cli.js build --config examples/spa-angular/dvhb.config.js --verbose --app-env=development",
    "gh-pages": "gh-pages -d examples/basic/dist",
    "pregh-pages": "ASSET_PATH=\"https://dvhb.github.io/webpack/\" npm run build:basic",
    "phantomjs": "phantomjs test/phantom.js examples/basic/dist/index.html",
    "gulp:templates": "node_modules/gulp/bin/gulp.js templates --gulpfile gulp/gulpfile.js --app-env=development --config ../examples/spa-angular/dvhb.config.js",
    "gulp:gzip": "node_modules/gulp/bin/gulp.js gzip --gulpfile gulp/gulpfile.js --app-env=development --config ../examples/spa-angular/dvhb.config.js"
  },
  "keywords": [
    "webpack",
    "dvhb",
    "generator",
    "starter",
    "kit"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
