    "svgo-loader": "^1.2.1",
    "webpack": "^3.4.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-config-loader": "^0.1.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-manifest-plugin": "^1.2.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^4.1.0",
    "yeoman-environment": "1.6.3",
    "yeoman-generator": "0.24.1"
  },
  "devDependencies": {
    "cross-env": "~3.1.3",
    "gh-pages": "~0.11.0",
    "lint-staged": "~3.2.5",
    "mock-require": "~1.3.0",
    "yeoman-test": "^1.5.0"
  },
  "scripts": {
    "init": "bin/webpack.js init",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:init": "node fixture.js",
    "start": "npm run dev:basic",
    "dev:basic": "bin/webpack.js server --config examples/basic/dvhb.config.js --verbose",
    "dev:static-site": "bin/webpack.js server --config examples/static-site/dvhb.config.js --verbose",
    "dev:spa-angular": "bin/webpack.js server --config examples/spa-angular/dvhb.config.js --verbose",
    "start:static-site": "bin/webpack.js server --config examples/static-site/dvhb.config.js --env=production",
    "prestart:static-site": "npm run build:static-site",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "lint-staged": "lint-staged",
    "build": "bin/webpack.js build --config examples/basic/dvhb.config.js --verbose --app-env=development",
    "build:static-site": "bin/webpack.js build --config examples/static-site/dvhb.config.js --verbose --app-env=development",
    "build:spa-angular": "bin/webpack.js build --config examples/spa-angular/dvhb.config.js --verbose --app-env=development",
    "gh-pages": "gh-pages -d examples/basic/dist",
    "pregh-pages": "ASSET_PATH=\"https://dvhbru.github.io/webpack/\" npm run build",
    "phantomjs": "phantomjs test/phantom.js examples/basic/dist/index.html",
    "gulp:templates": "node_modules/gulp/bin/gulp.js templates --gulpfile gulp/gulpfile.js --config ../examples/spa-angular/dvhb.config.js"
  },
  "pre-commit": "lint-staged",
  "keywords": [
    "webpack",
    "dvhb",
    "generator",
    "starter",
    "kit"
  ]
}
