    "commands",
    "scripts",
    "gulp",
    "gulpfile.js",
    ".eslintrc"
  ],
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "@dvhb/eslint-config": "*",
    "autoprefixer": "^6.7.4",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "csso-loader": "^0.2.1",
    "eslint": "^3.16.1",
    "eslint-loader": "^1.9.0",
    "express": "^4.14.1",
    "express-pug": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "findup": "^0.1.5",
    "git-revision-webpack-plugin": "^2.4.1",
    "gulp": "git://github.com/gulpjs/gulp.git#4.0",
    "gulp-filter": "^5.0.0",
    "gulp-pug": "^3.2.0",
    "html-webpack-plugin": "^2.29.0",
    "ip": "^1.1.5",
    "json-loader": "^0.5.4",
    "jstransformer-markdown-it": "^2.0.0",
    "lodash": "^4.17.4",
    "minimatch": "^3.0.2",
    "minimist": "1.2.0",
    "modernizr": "^3.3.1",
    "modernizr-loader": "^1.0.1",
    "postcss": "^5.2.14",
    "postcss-loader": "^1.3.1",
    "postcss-svg": "^1.0.6",
    "prettyjson": "^1.2.1",
    "pug": "^2.0.0-beta3",
    "pug-loader": "^2.3.0",
    "raw-loader": "^0.5.1",
    "require-dir": "^0.3.1",
    "semver-utils": "^1.1.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
