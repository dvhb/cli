  "peerDependencies": {
    "webpack": "1"
  },
  "devDependencies": {
    "babel-eslint": "~7.1.0",
    "babel-register": "~6.18.0",
    "cross-env": "~3.1.3",
    "eslint": "^3.15.0",
    "gh-pages": "~0.11.0",
    "lint-staged": "~3.2.5",
    "mock-require": "~1.3.0",
    "webpack": "1.13.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run dev:basic",
    "dev:basic": "bin/webpack.js server --config examples/basic/dvhb.config.js --verbose",
    "dev:static-site": "bin/webpack.js server --config examples/static-site/dvhb.config.js --verbose",
    "start:static-site": "bin/webpack.js server --config examples/static-site/dvhb.config.js --env=production",
    "prestart:static-site": "npm run build:static-site",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "lint-staged": "lint-staged",
    "build": "bin/webpack.js build --config examples/basic/dvhb.config.js",
    "build:static-site": "bin/webpack.js build --config examples/static-site/dvhb.config.js",
    "gh-pages": "echo \"Error: no gh-pages specified\" && exit 1",
    "phantomjs": "phantomjs test/phantom.js examples/basic/dist/index.html"
  },
  "pre-commit": "lint-staged",
  "keywords": [
    "webpack",
    "dvhb",
    "generator",
    "starter",
    "kit"
  ]
}
