    "css-loader": "^0.26.1",
    "csso-loader": "^0.2.1",
    "express": "^4.14.1",
    "express-pug": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "findup": "^0.1.5",
    "html-webpack-plugin": "^2.28.0",
    "lodash": "^4.17.4",
    "minimist": "1.2.0",
    "postcss": "^5.2.14",
    "postcss-loader": "^1.3.1",
    "prettyjson": "^1.2.1",
    "pug": "^2.0.0-beta3",
