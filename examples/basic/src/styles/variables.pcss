/* Colors */
$cBlack: #000000;
$cWhite: #ffffff;
$cGreen: #8ac942;
$cRed: #fa6464;
$cGrey: #999999;

$transition: 'all .3s ease';

/* media queries */
$xs: '(max-width: 767px)';
$sm: '(min-width: 768px) and (max-width: 991px)';
$md: '(min-width: 992px) and (max-width: 1599px)';
$lg: '(min-width: 1600px)';
